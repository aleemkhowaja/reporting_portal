<struts>

    
	<!-- Some or all of these can be flipped to true for debugging -->
	<!-- <constant name="struts.i18n.reload" value="false" /> -->
	<constant name="struts.devMode" value="true" />
	<constant name="struts.ui.theme" value="path-xhtml" />
	<constant name="struts.multipart.maxSize" value="2147483648" /> <!-- 2GB max Whole size upload -->
	<!--
	
	
		<constant name="struts.configuration.xml.reload" value="false" />
		<constant name="struts.action.extension" value="action,," />
	-->
	<constant name="struts.serve.static.browserCache" value="false" />
	
	<!--  regular expression is for struts 2.5 upgrade to avoid js and images to path through interceptor -->
	<constant name="struts.action.excludePattern" value="/,\S+(?&lt;!\/struts\/utils|\\struts\\utils)\.js$,\S+\.css$,\S+\.jpeg$,\S+\.jpg$,\S+\.png$,/path/AlertsAjaxServlet/*,/path/BatchProcessingServlet/*,/path/UpgradeServlet/*,/servlets/image,/path/notificationsWS.*"/>	
	<constant name="struts.mapper.class" value="com.path.struts2.redirectaction.PathDefaultActionMapper" />
	<constant name="struts.ognl.allowStaticMethodAccess" value="true"/>
	
	<package name="path-default" extends="struts-default,json-default">
		<result-types>
		<result-type name="redirectAction" class="com.path.struts2.redirectaction.PathServletActionRedirectResult"></result-type>
		<result-type name="json" class="com.path.struts2.json.PathJSONResult">
		<!-- To Exclude all properties that are null value to be returned in JSON Response Object -->
  		  <param name="excludeNullProperties">true</param>
  		  <!-- compressing the Response of JSON with GZIP, works only if request Accepts gzip
  		  but this causes errors in JSON Response return  
  		  <param name="enableGZIP">true</param>
  		  -->
		</result-type>
		</result-types>
	
		<interceptors>
		    <interceptor name="fileUpload" class="com.path.lib.common.interceptor.PathFileUploadInterceptor"></interceptor>
		    <interceptor name="path-http_session" class="com.path.lib.common.interceptor.PathSessionInterceptor"></interceptor>
			 
			<!-- <interceptor name="path-parameters" class="com.opensymphony.xwork2.interceptor.ParametersInterceptor"></interceptor> -->
			
			
			<interceptor-stack name="pathDefaultStack">
			<!-- <interceptor-ref name="defaultStack"></interceptor-ref>  -->
			<!-- for details of each interceptor refer to http://struts.apache.org/2.2.1/docs/interceptors.html -->
				<!-- interceptor to check if user still logged in or forced to logout -->
				
				<interceptor-ref name="exception"/>
               <!--   
                Makes the previous Action's properties available to the current Action. Commonly used together with <result type="chain"> (in the previous Action). 
                <interceptor-ref name="alias"/> 
                -->
                <interceptor-ref name="servletConfig"/>
                <!-- session interceptor checking to be applied after servletConfig to get use of data available in session -->
                <interceptor-ref name="path-http_session"/>
                <interceptor-ref name="i18n"/>
               <!--
                 If the Action implements Preparable, calls its prepare method. 
                 <interceptor-ref name="prepare"/>
                 <interceptor-ref name="chain"/> 
                 -->
                 <!-- 
                <interceptor-ref name="debugging"/>
                 -->
               <!-- <interceptor-ref name="scopedModelDriven"/> -->
                <interceptor-ref name="modelDriven"/>
                <!-- 	 An Interceptor that adds easy access to file upload support. 
                <interceptor-ref name="fileUpload"/>
                -->
                <interceptor-ref name="checkbox"/>
                <interceptor-ref name="multiselect"/>
                <!-- Sets the struts.xml defined parameters onto the action. These are the <param> tags that are direct children of the <action> tag.  
                <interceptor-ref name="staticParams"/>
                -->
                <interceptor-ref name="actionMappingParams"/>
            
                <interceptor-ref name="params"/> 
                  
                <interceptor-ref name="conversionError"/>
                <!-- 
                <interceptor-ref name="validation">
                       <param name="excludeMethods">*</param>
                    <param name="includeMethods">submit*</param>
                </interceptor-ref>
                 -->
                <!-- 
                <interceptor-ref name="workflow">
                    <param name="excludeMethods">*</param>
                    <param name="includeMethods">submit*</param>
                </interceptor-ref>
                 -->
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="pathDefaultStack"/>
		
		<global-results>
		   <result name="SUCCESS_DYNAMIC_PARAM" type="freemarker">/WEB-INF/pages/technical/dynamicparams/DynamicParams.ftl</result>
		   <result name="SUCCESS_STATUS">/WEB-INF/pages/common/status/StatusList.jsp</result>
		   <result name="ERROR_ACTION">/login/ErrorAction.jsp</result>
		   <result name="APP_MAIN_CTXROOT">/</result>	
		   <result name="error">/login/ErrorPage.jsp</result>
		   <result name="session_checking">/WEB-INF/pages/appcomponents/common/LoginSessionChecking.jsp</result>
		   <result name="login_alert_redirection">/login/LoginAlertRedirection.jsp</result>
		   <result name="login_comp_br">/WEB-INF/pages/appcomponents/desktop/LoginCompBr.jsp</result>
		   <!-- redirecting to force logout method in UnsecureAction with different package to overcome accessing interceptor path-http-session -->
		   <result name="forced_logged_out" type="redirectAction">
			    <param name="actionName">forceLogoutAction</param>
				<param name="namespace">/forcelogout</param>
		   </result>
  		   <result name="loginToLandingPage" type="redirectAction">
		   		<param name="actionName">dashboard</param>
				<param name="namespace">/pathdesktop</param>
		   </result>
		             
             <result name="loadExternalScreenErrorPage" type="redirectAction">
             	<param name="actionName">targetScreenErrorAction</param>
             </result>
             
            <result name="loadDestinationScreen" type="redirectAction">
				<param name="actionName">commonDecoration</param>
            	<param name="namespace">/</param>
            	<param name="_pageRef">${_pageRef}</param>
            	<param name="isRTL">${isRTL}</param>            	
            	<param name="language">${language}</param>
            	
            	<param name="appId">${appId}</param>
            	<param name="favoriteId">${favoriteId}</param>
            	<param name="originalProgRef">${originalProgRef}</param>
            	<param name="originalAppUrl">${originalAppUrl}</param>
            	<param name="originalAppName">${originalAppName}</param>
            	<param name="externalScreen">${externalScreen}</param>
            	<param name="destinationScreenUrl">${destinationScreenUrl}</param>
            	<param name="destinationProgRef">${destinationProgRef}</param>
            	<param name="additionalParams">${additionalParams}</param>
            	
            	<!-- additional parameters defined in DesktopAction fillOtherCommonParams() -->
            	<param name="isArbMnthEng">${isArbMnthEng}</param>
            	<param name="printReportAsPDF">${printReportAsPDF}</param>
            	
			</result> 	
             
             <result name="SUCCESS_JSON" type="json">
					<param name="ignoreHierarchy">false</param> 
			</result>
		 	<result name="SUCCESS_STRING" type="stream">	    	  			
				  <param name="contentType">text/plain</param>
				  <param name="inputName">inputStream</param>
			 </result>
			 <result name="JSON_TREE" type="json">
			 	<param name="ignoreHierarchy">false</param> 
			 	<param name="root">nodes</param>
			 </result>
			 <!-- used for token failure -->
			 <result name="redirectLogin" type="redirectAction">
				<param name="method">login</param>
				<param name="actionName">loginAction_</param>
				<param name="namespace">/login</param>
			 </result>	
			 
			 <result name="excel" type="stream">
				 <param name="contentType">application/vnd.openxmlformats-officedocument.spreadsheetml.sheet</param>
				 <param name="contentDisposition">attachment; filename="${streamFileName}"</param>
				 <param name="bufferSize">1024</param> 
				 <param name="inputName">streamExcel</param> 
			</result>
			<result name="excel_error" type="stream">
				 <param name="contentType">text/plain</param>
				 <param name="inputName">streamExcel</param> 
			</result>
			
			<result name="doc" type="stream">
				 <param name="contentType">application/octet-stream</param>
				 <param name="contentDisposition">attachment; filename="${streamFileName}"</param>
				 <param name="bufferSize">1024</param> 
				 <param name="inputName">streamDoc</param> 
			</result>
			<result name="doc_error" type="stream">
				 <param name="contentType">text/plain</param>
				 <param name="inputName">streamDoc</param> 
			</result>
						
			<result name="css" type="stream">
				 <param name="contentType">text/css</param>
				 <param name="contentDisposition">attachment; filename="themeCustomization.css"</param>
				 <param name="bufferSize">1024</param> 
				 <param name="inputName">cssStream</param> 
			</result>
			<result name="returnImage" type="stream">
				 <param name="contentType">image/png</param>
				 <param name="contentDisposition">attachment; filename="image.png"</param>
				 <param name="bufferSize">1024</param> 
				 <param name="inputName">imageStream</param> 
			</result>
			<result name="returnImage_error" type="stream">
				 <param name="contentType">text/plain</param>
				 <param name="inputName">imageStream</param> 
			</result>
			 		
		</global-results>
		
		<action name="targetScreenErrorAction"  class="desktopAction" method="loadExternalScreenErrorPage">
