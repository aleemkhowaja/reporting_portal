	<!-- used to avoid the interceptor in case of force logout, to not pass by httpSession interceptor-->
	<package name="forcelogout" namespace="/forcelogout" extends="struts-default">
		<action name="forceLogoutAction" method="forcedLoggedOut"
			class="unSecureAction">
			<result name="post_login" type="redirectAction">
				<param name="actionName">unSecureAction_prelogin?request_locale=${languages}</param>
				<param name="namespace">/login</param>
			</result>
		</action>
	</package>
		<!-- used dummy call to the server in case need to know if http session is still alive-->
	<package name="dummy" namespace="/path/dummy" extends="path-default">
		<action name="DummyAction" method="checkAlive" class="desktopAction">
		 	<result name="JSON_DUMMY" type="json">
			 <!-- additionalParams is just any attribute to be returned as empty to have properly rendered at client side-->
			 	<param name="root">additionalParams</param>
			 </result>	
		</action>
		<!-- used in landing page favorite/application/workspace to reload session details like running date to be taken into consideration before clicking the workspace/favorite/application button -->
		<action name="SessionDetails" method="returnSessionDetails" class="desktopAction">
		 	<result name="jsonSuccess" type="json">
				<param name="ignoreHierarchy">false</param> 
			 </result>	
		</action>
		
	</package>
