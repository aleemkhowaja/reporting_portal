/* readonly/disabled inputs*/
input[readonly],input[disabled],textarea[readonly]
{ 
	border : 1px solid #999999 !important;
	color:#3C3C3C !important;
}
/* readonly/disabled inputs*/
input[readonly]:not(.pathHighlights),input[disabled]:not(.pathHighlights),textarea[readonly]:not(.pathHighlights)
{ 
	background:#E1E1E1  !important;
}
input[mode="number"]
{
  text-align:right !important;
}
/*Context Menu*/
.context-menu {
    background-color: #F2F2F2;
    border: 1px solid #74b2e2;
    list-style-type: none;
    /*margin: 0;*/
    padding: 0;
}
.context-menu a
{
    color: #0070a3;
    text-decoration: none;
}
.context-menu a:hover {
    background-color: #FFD47F;
    color: #0070a3;
    text-decoration: none;
}
.icon-context-menu
{
  display:inline-block !important;
  height:12px !important
}

.pathHighlights
{
	background: #FFFF7F !important;
}
.checkboxHighlights
{
	outline: 4px #FFFF7F solid;
}
