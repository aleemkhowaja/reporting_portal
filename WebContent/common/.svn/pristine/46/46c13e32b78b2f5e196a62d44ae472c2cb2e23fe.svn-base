.accordionmenu{
padding:0;
width: 99.9%; /*width of accordion menu*/
}

.accordionmenu .accordionheader{ /*CSS class for menu headers in general (expanding or not!)*/
font: 11px Arial;

/*background: black url(../../js/menu/img/Bg.png) repeat-x center left;*/
margin-bottom: 0px; /*bottom spacing between header and rest of content*/
margin-top:1px;
padding: 4px 0 4px 22px; /*header text is indented 10px*/
cursor: hand;
cursor: pointer;
margin-left: -20px;
 
}

.accordionmenu .openedrootheader{ /*CSS class to apply to expandable header when it's expanded*/
/*background-image: url(../../js/menu/img/Bg.png);*/
} 

.accordionmenu ul{ /*CSS for UL of each sub menu*/
/*background-color:#E0E6EC; */

font-weight:20;
list-style-type: none;
margin: 0;
padding: 0;
margin-top:0px;


}

.accordionmenu ul .accordionheader{ 
	border-bottom:solid 1px black;
	}
	
.accordionmenu ul li{
padding-bottom: 1px; /*bottom spacing between menu items*/
margin-left: 10px;
}



.accordionmenu ul li a{
color: #000000;
display: block;
padding: 2px 0px; 
padding-left: 5px; /*link text is indented 19px*/
text-decoration: none;
/*border-bottom: 1px solid #dadada; */
font: 8pt Tahoma;
}

.accordionmenu ul li a:hover{ /*hover state CSS*/
background-color: #99ADD6 ;
}


#arrowbulletImgId
{
	padding-right:5px;
	/*src:url('arrowbullet.png');*/
}

.accordionmenu ul li a:visited{
 color:#000000 ;
  
}



.accordionmenu h3 a{	
font:11px Tahoma;

text-decoration: none;
}
.accordionmenu .rootcontent
{
	
	border-top:none;
	 
}

.leafHeaderTitle
{
	padding-left: 5px;
	
}
rightIcon{
	background-image: url("jquery-ui-images/ui-icons_6da8d5_256x240.png");
	background-position: -32px -16px;
	
	}
.accordprefix{
	margin-top:-1px;
	float:left;
	padding-right: 2px;
	
	}
.accordsuffix{
	float:right
	}	
	
.loading{	
	background-image: url("../../images/loadingMenu.gif");
	display:block;
    height: 16px;
    width: 16px;
	
	}
.tableProp{
	border-collapse: collapse;
	border-spacing: inherit;
	border: none;
	margin: -2px;
}
 